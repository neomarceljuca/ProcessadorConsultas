	"|X|tipoconta.idtipoconta=contas.tipoconta_idtipoconta" -> "|X|usuario.idusuario=contas.usuario_idusuario"
	"|X|usuario.idusuario=contas.usuario_idusuario" -> "piidusuario,usuario.idusuario"
